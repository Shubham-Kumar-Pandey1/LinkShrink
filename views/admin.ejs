<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Welcome Page</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        h1 {
            text-align: center;
        }
        table {
            width: 80%;
            margin: 20px auto;
            border-collapse: collapse;
        }
        th, td {
            padding: 10px;
            text-align: left;
            border: 1px solid #ddd;
        }
        th {
            background-color: #f2f2f2;
        }
        tr:nth-child(even) {
            background-color: #f9f9f9;
        }
        tr:hover {
            background-color: #e2e2e2;
        }
    </style>
</head>
<body>
    <h1>Hello from Developer</h1>
    <table>
        <tr>
            <th>Index</th>
            <th>_id</th>
            <th>shortID</th>
            <th>redirectUrl</th>
            <th>__v</th>
        </tr>
        <% allData.forEach((element, index) => { %>
            <tr>
                <td><%= index + 1 %>.</td>
                <td><%= element._id %></td>
                <td><%= element.shortID %></td>
                <td><%= element.redirectUrl %></td>
                <td><%= element.__v %></td>
            </tr>
        <% }) %>
    </table>
</body>
</html>
